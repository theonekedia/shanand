<p id="notice"><%= notice %></p>

<h1>Listing Script Meta Data</h1>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th> ISIN </th>
      <th> BSE </th>
      <th> NSE </th>
      <th> INDUSTRY </th>
      <th> 52 Week Low </th>
      <th> 52 Week High </th>
    </tr>
  </thead>

  <tbody>
    <% @script_meta_data.each do |script_meta_datum| %>
      <tr>
		<td><%= script_meta_datum.isi_num %></td>
		<td><%= script_meta_datum.bse_script_numb %></td>
		<td><%= script_meta_datum.nse_script_name %></td>
		<td><%= script_meta_datum.industry %></td>
		<td><%= script_meta_datum.f2_week_low.round(2) %></td>
		<td><%= script_meta_datum.f2_week_high.round(2) %></td>
		      
        <td><%= link_to 'Show', script_meta_datum %></td>
        <td><%= link_to 'Edit', edit_script_meta_datum_path(script_meta_datum) %></td>
        <td><%= link_to 'Destroy', script_meta_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script meta datum', new_script_meta_datum_path %>
