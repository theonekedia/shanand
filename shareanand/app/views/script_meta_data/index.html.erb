<p id="notice"><%= notice %></p>

<h2>Script Meta Data</h2>
<!--div class="pagination"-->
<%= will_paginate @script_meta_data %>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th> ISIN </th>
      <th> BSE </th>
      <th> NSE </th>
      <th> INDUSTRY </th>
      <th> 52 wk Low </th>
      <th> 52 wk High </th>
      <th> No.of Shares </br> <span font-size="6"> (in Cr) </span></th>
      <th> <acronym title="Graham Fair Value Price"> GFVP </acronym></th>
      <th> Book Value </th>
      <th> EPS </th>
     
    </tr>
  </thead>

  <tbody>
    <% @script_meta_data.each do |script_meta_datum| %>
      <tr>
		<td><%= script_meta_datum.isi_num %></td>
		<td><%= script_meta_datum.bse_script_numb %></td>
		<td><%= link_to script_meta_datum.nse_script_name, company_details_path(script_meta_datum.nse_script_name) %></td>
		<td><%= link_to script_meta_datum.industry, industry_detail_path(script_meta_datum.industry) %></td>
		<td class="text-right"><%= script_meta_datum.f2_week_low.round(2) %></td>
		<td class="text-right"><%= script_meta_datum.f2_week_high.round(2) %></td>
    <td class="text-right"><%= script_meta_datum.number_of_shares %></td>
    <td class="text-right"><%= script_meta_datum.graham_fvp.round(2) %></td>
    <td class="text-right"><%= script_meta_datum.book_value %></td>
    <td class="text-right"><%= script_meta_datum.eps %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @script_meta_data %>
<br>

<%= link_to 'New Script meta datum', new_script_meta_datum_path %>
