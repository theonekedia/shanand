  <h3> Compititors </h3>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th> Company </th>
      <th> 52 Week Low </th>
      <th> Book Value </th>
      <th> Graham Fair Value Price </th>
      <th> Latest Price </th>
      <th> 52 Week High </th>
      <th> Volume </th>   
      <th> EPS </th>
      <th> % Chg </th>
      <th> Proximity (%) </th>
      <th> Market Cap (Cr)</th>

    </tr>
  </thead>

  <tbody>
    <% @compititors.each do |compititor| %>
      <tr>
		<td><%= link_to compititor.nse_script_name, company_details_path(compititor.nse_script_name) %></td>
		<td><%= compititor.f2_week_low.round(2) %></td>
    <td><%= compititor.book_value %></td>
    <td><%= compititor.graham_fvp.round(2) %></td>
    <td><%= compititor.today_close %></td>
		<td><%= compititor.f2_week_high.round(2) %></td>
    <td><%= compititor.tot_shares_traded %></td>
    <td><%= compititor.eps %></td>  
    <td><%= compititor.perc_change.round(2) %></td>  
    <td><%= ((compititor.today_close - compititor.f2_week_low) / (compititor.f2_week_high - compititor.f2_week_low)).round(2) %></td>  
    <td><%= (compititor.market_cap / 100).round(2) %></td>  

      </tr>
    <% end %>
  </tbody>
</table>
